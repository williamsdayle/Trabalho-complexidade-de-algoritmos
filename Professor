package trabalhoanalisealg;

public class Professor {

    String nome;
    int cargaHoraria[] = new int[5];
    
    
    public Professor(String nome)
    {
        this.nome = nome;
        for(int i = 0; i < 5; i++)
        {
            this.cargaHoraria[i] = 8;
        }
                
    }

    
}
